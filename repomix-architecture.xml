            head.addEventListener('click', () => {
              const open = item.hasAttribute('open');
              item.toggleAttribute('open');
              head.setAttribute('aria-expanded', String(!open));
              head.querySelector('span[aria-hidden]').textContent = !open ? '‚ñæ' : '‚ñ∏';
